{
  "name": "web-scaffold",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.8.7",
    "@sentry/react": "^7.33.0",
    "@sentry/tracing": "^7.33.0",
    "@tanstack/react-query": "^4.2.3",
    "@types/json-merge-patch": "^0.0.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.7.13",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "json-merge-patch": "^1.0.2",
    "moment": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-onesignal": "^2.0.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typeface-lato": "^1.1.13",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-app-env --env-file=envConfig/${BUILD_ENV}.env start",
    "local": "react-app-env --env-file=envConfig/local.env start",
    "build": "react-app-env --env-file=envConfig/${BUILD_ENV}.env build && echo '/* /index.html  200' | cat >build/_redirects ",
    "build:production": "BUILD_ENV=production yarn run build",
    "test": "react-app-env test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx}",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json,scss}' --config ./.prettierrc",
    "husky-install": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "process": "0.11.10",
    "react-app-env": "^1.2.3",
    "tsc-files": "^1.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  }
}
